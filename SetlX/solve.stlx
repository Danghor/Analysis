// compute the sequence (x_n)-N defined inductively as follows:
//  1. x_1     := 1.0,
//  2. x_{n+1} := cos(x_n)
solve := procedure(k) {
    x    := [];  // x[n] stores x_{n}
    x[1] := 1.0;
    for (n in [1 .. k]) {
        x[n+1] := cos(x[n]);
        print("x_{$n$} = $x[n+1]$");
    }
};

solve(99);

