 enabled="false" first-number="1" first-numbered-page="1" horizontal-location="right" style="Page Number" vertical-location="bottom"/><Group><Input><Text-field layout="Normal" style="_cstyle257"><Font family="Lucida Bright">Arbeitsblatt zu Fourier-Reihen - Mathematik (1)</Font></Text-field></Input></Group><Section><Title><Text-field layout="Heading 1" style="Heading 1"><Font family="Lucida Bright">Fourier-Reihen</Font></Text-field></Title><Group><Input><Text-field layout="Normal" prompt="&gt; " style="Maple Input">restart; with(plots):</Text-field></Input><Output><Text-field layout="Warning" style="Warning">Warning, the name changecoords has been redefined</Text-field></Output></Group><Section><Title><Text-field layout="Heading 2" style="Heading 2"><Font family="Lucida Bright">Prozedur zur Berechnung der Fourier-Koeffizenten und Grafik</Font></Text-field></Title><Group><Input><Text-field layout="Normal" style="Normal"><Font encoding="ISO8859-1" family="Lucida Bright">Achtung: In der vorliegenden Form \374berpr\374ft die Prozedur die \374bergebenen Parameter </Font></Text-field><Text-field layout="Normal" style="Normal"><Font family="Lucida Bright">noch nicht!</Font></Text-field><Text-field layout="Normal" style="Normal"><Font encoding="ISO8859-1" family="Lucida Bright">Au\337erdem sucht Maple nach einer geschlossenen L\366sung f\374r die auftretenden Integrale. </Font></Text-field><Text-field layout="Normal" style="Normal"><Font family="Lucida Bright">Eine (denkbare) numerische Auswertung der Integrale findet bei der Berechnung der </Font></Text-field><Text-field layout="Normal" style="Normal"><Font family="Lucida Bright">Koeffizienten nicht statt, sondern erst bei einer eventuellen Grafik.</Font></Text-field></Input></Group><Group><Input><Text-field layout="Normal" prompt="&gt; " style="Maple Input">Fourierkoeff := proc(left, right, func, nmax)
# Berechnung der Fourierkoeffizienten 
# Parameter:
#   left, right   Periodenintervall
#   func          Funktion
#   nmax          Berechnung der Koeffz. bis zum Index nmax
# optional:
#   plot=true     Grafik zeichnen (default: false)  <Font encoding="ISO8859-1">
#   last=true     alle N\344herungen einzeichnen (default: nur die</Font>
#                                                          letzte)

global a, b;
local T, omega, i, Options, Defaults, graph, onlylast, P;  

# optionale Parameter auswerten
Defaults := table([
  PositionalParms = table([]),
  OptionParms = table([ 
    'plot' = [false, 'truefalse'],
    'last' = [false, 'truefalse']  
    ])
]);
Options := table();
ProcessOptions( 4, [args], Defaults, Options): 
# print(Options); # (nur zur Fehlersuche)
graph := Options[plot]:
onlylast  := Options[last]:
 <Font encoding="ISO8859-1">
T := right-left;  # Periodenl\344nge</Font>
omega := 2*Pi/T;  # Kreisfrequenz

# Berechnung der Koeffizienten
a[0] := 2/T*int(func(x), x=left..right);
for i from 1 to nmax by 1 do
  a[i] := 2/T*int(func(x)*cos(i*omega*x), x=left..right);
  b[i] := 2/T*int(func(x)*sin(i*omega*x), x=left..right);
end do;

# Ausgabe der Koeffizienten
printf("a[0] = %12a\n", a[0]);
for i from 1 to nmax by 1 do
  printf("a[%d] = %12a   \t", i, a[i]);
  printf("b[%d] = %12a\n", i, b[i]);
end do;

# Grafik
if (graph) then 
  P[-1] := plot(func(x), x=left..right, color=green, thickness=2):
  if (onlylast) then
    P[0] :=  plot(a[0]/2+sum(a[k]*cos(k*omega*x)+
             b[k]*sin(k*omega*x),k=1..nmax), x=left..right): 
    display(P[-1], P[0]);
  else 
    P[0] := plot(a[0]/2, x=left..right):
    for i from 1 to nmax by 1 do
      P[i] := 
        plot(a[0]/2+sum(a[k]*cos(k*omega*x)+b[k]*sin(k*omega*x),k=1..i),
                               x=left..right): 
    end do;
    display(seq(P[i], i=-1..nmax));
  fi; 
fi;

end proc:</Text-field></Input></Group></Section><Section><Title><Text-field layout="Heading 2" style="Heading 2"><Font family="Lucida Bright">Anwendung 1 (Rechteck)</Font></Text-field></Title><Group><Input><Text-field layout="Normal" style="ParagraphStyle1"><Font encoding="ISO8859-1" family="Lucida Bright" style="_cstyle259">Rechteckschwingung: "st\374ckweise" Definition mittels </Font><Font family="Lucida Bright" style="_cstyle260">piecewise():</Font></Text-field></Input></Group><Group><Input><Text-field layout="Normal" prompt="&gt; " style="Maple Input">f := x -&gt; piecewise(0&lt;=x and x&lt;=Pi, 1, Pi&lt;x and x&lt;=2*Pi, -1);</Text-field></Input><Output><Text-field layout="Maple Output" style="2D Output"><Equation>NiM+SSJmRzYiZio2I0kieEdGJUYlNiRJKW9wZXJhdG9yR0YlSSZhcnJvd0dGJUYlLUkqcGllY2V3aXNlR0kqcHJvdGVjdGVkR0YuNiYzMSIiITkkMUYzSSNQaUdGLiIiIjMyRjVGMzFGMywkRjUiIiMhIiJGJUYlRiU=</Equation></Text-field></Output></Group><Group><Input><Text-field layout="Normal" prompt="&gt; " style="Maple Input">plot(f(x), x=0..2*Pi);</Text-field></Input><Output><Text-field layout="Maple Plot"><Plot height="400" plot-scale="1.0" plot-xtrans="0.0" plot-ytrans="0.0" type="two-dimensional" width="400"></Plot></Text-field></Output></Group><Group><Input><Text-field layout="Normal" style="_cstyle261"><Font family="Lucida Bright">Berechnung der Fourierkoeffizienten  und Grafik aller Partialsummen der Reihe bis n=6:</Font></Text-field></Input></Group><Group><Input><Text-field layout="Normal" prompt="&gt; " style="Maple Input">Fourierkoeff(0, 2*Pi, f, 6, plot=true, last=false);</Text-field></Input><Output><Text-field layout="Normal" style="Line Printed Output">a[0] =            0
